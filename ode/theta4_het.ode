# n excitatory, n inhibitory theta neurons
# 

x[0..3]'=((1-cos(x[j]))+(1+cos(x[j]))*(a1+b1*sx-c1*sy))*pi
y[0..3]'=((1-cos(y[j]))+(1+cos(y[j]))*(a2+b2*sx-c2*sy))*pi

sx'=-eps*sx/mux
sy'=-eps*sy/muy

global 1 x[0..3]-pi {x[j]=-pi;sx=sx+al*eps/(4*mux)}
global 1 y[0..3]-pi {y[j]=-pi;sy=sy+al*eps/(4*muy)}

init sx=1,sy=1.03
x[0..3](0)=1
y[0..3](0)=1

p al=1
p a1=.5,b1=7,c1=6.5
p a2=1.1,b2=25,c2=25.1

p mux=1,muy=1
p eps=.01

@ dt=.01
@ total=1000
@ xp=sx,yp=sy
@ xlo=.9,xhi=1.1
@ ylo=.9,yhi=1.1
